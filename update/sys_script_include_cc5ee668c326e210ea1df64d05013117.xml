    <sys_script_include action="INSERT_OR_UPDATE">
        <access>package_private</access>
        <active>true</active>
        <api_name>x_58872_needit.getEmailScript</api_name>
        <caller_access>1</caller_access>
        <client_callable>true</client_callable>
        <description>Script include to get selected user's email from the database.</description>
        <mobile_callable>false</mobile_callable>
        <name>getEmailScript</name>
        <sandbox_callable>false</sandbox_callable>
        <script><![CDATA[var getEmailScript = Class.create();
getEmailScript.prototype = Object.extendsObject(global.AbstractAjaxProcessor, {

	getEmail : function(){
		var gr = new GlideRecord('sys_user');
		gr.addQuery('sys_id',this.getParameter('sysparm_userID'));
		gr.query();
		if(gr.next()){
			return gr.email+'';
		}
	},

    type: 'getEmailScript'
});]]></script>
